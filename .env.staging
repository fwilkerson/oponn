# Staging Environment Defaults (Local Docker Services)
OPONN_ENV=staging
OPONN_SKIP_CSRF=false

DATABASE_URL=postgresql+asyncpg://oponn_user:oponn_password@localhost:5432/oponn_db
REDIS_URL=redis://localhost:6379/0

# Use the stable LocalStack alias created by manage.py infra up
OPONN_KMS_KEY_ID=alias/oponn-local-key
LOCALSTACK_ENDPOINT=http://localhost:4566
